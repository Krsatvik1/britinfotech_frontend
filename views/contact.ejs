<%- include('partials/header' , {dataSEO , dataSEOKeywords , strapi_base, base_url}) %> <%- include('partials/nav', {dataContact:data1}) %>
        <header class="mx-6 md:mx-24 pt-10 md:pt-[2.25rem]">
            <div class="">
                <h1
                    class="font-crash home-title md:font-normal font-medium text-[1.375rem] md:text-header basis-full md:basis-auto">
                    <%= data1.data.attributes.Heading %>
                </h1>
                <p class="text-base md:text-lg md:max-w-[60rem] md:mt-4 mt-8">
                    <%= data1.data.attributes.Paragraph %>
                </p>
            </div>
        </header>
        <section
            class="mx-6 md:mx-24 mt-10 md:mt-[2.25rem] md:rounded-[1.25rem] rounded-[0.75rem] overflow-hidden bg-[linear-gradient(89deg,#0B669D_62.52%,#00E5BC_129.3%)] md:flex">
            <div class="md:rounded-r-[1.25rem] rounded-b-[0.75rem] md:w-[60%] rounded-r-[0.75rem] overflow-hidden md:pb-0 pb-10 bg-white md:p-16">
                <form id="form" class="space-y-10" method="POST" action="/contact/submit">
                    <div>
                        <label for="name"  class="block text-[#969696] text-sm">Name*</label>
                        <input id="name" required name="contact[name]" type="text"
                            class="w-full block border-b-[1px] active:outline-none border-black md:text-lg text-base" />
                        <p class="text-red-500 opacity-0">Please enter corret value</p>
                    </div>
                    <div>
                        <label for="company" class="block text-[#969696] text-sm">Company Name</label>
                        <input id="company" name="contact[company]" type="text"
                            class="w-full block border-b-[1px] active:outline-none border-black md:text-lg text-base" />
                            <p class="text-red-500 opacity-0">Please enter corret value</p>
                    </div>
                    <div>
                        <label for="email" class="block text-[#969696] text-sm">Email*</label>
                        <input id="email" required name="contact[email]" type="email"
                            class="w-full block border-b-[1px] active:outline-none border-black md:text-lg text-base" />
                            <p class="text-red-500 opacity-0">Please enter corret value</p>
                    </div>
                    <div>
                        <label for="mobile" class="block text-[#969696] text-sm">Phone Number*</label>
                        <input id="mobile" required name="contact[mobile]" type="tel" pattern="[+\d]*"
                            class="w-full block border-b-[1px] active:outline-none border-black md:text-lg text-base" />
                            <p class="text-red-500 opacity-0">Please enter corret value</p>
                    </div>
                    <div>
                        <label for="message" class="block text-[#969696] text-sm">How can we help you?</label>
                        <textarea id="message" name="contact[message]" type="text"
                            class="w-full block border-b-[1px] h-32 active:outline-none border-black md:text-lg text-base"></textarea>
                            <p class="text-red-500 opacity-0">Please enter corret value</p>
                    </div>
                    <div class="flex md:gap-6 gap-2">
                        <input type="checkbox" id="subscribe" name="contact[subscribe]"
                            class="border-b-[1px] active:outline-none border-black w-6 h-6" />
                        <label class="block md:text-base text-sm font-medium align-middle" for="subscribe">Subcribe to our newsletter for updates</label>
                    </div>
                    <div>
                        <button data-sitekey="6Ldez3UpAAAAAGX-faudz3vKoV4dI46MDIXFSKpI"
                        data-callback='onSubmit'
                        data-action='submit' class="g-recaptcha  rounded-full btn-30 text-center flex justify-center items-center h-12 w-44 font-normal">
                            <p class="text-container basis-full"><span class="text">Let's Talk</span></p>
                        </button>
                    </div>
                </form>
            </div>
            <div class="flex-grow md:py-16 px-4 py-8 md:px-8 text-white space-y-8">
                <h2 class="text-[2rem]">
                    More ways to contact us
                </h2>
                <div class="space-y-4">
                    <a href="tel:<%= data1.data.attributes.Phone_No%>" class="flex gap-8">
                        <div>
                            <svg xmlns="http://www.w3.org/2000/svg" class="h-full aspect-square" viewBox="0 0 21 21"
                                fill="none">
                                <path
