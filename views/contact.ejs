                            </svg>
                        </div>
                        <div>
                            <h3>
                                <%= data1.data.attributes.Timing%>
                            </h3>
                        </div>
                    </div>
                </div>

                <iframe src="<%= data1.data.attributes.Geo_Location_data  %>" style="border:0;" allowfullscreen=""
                    class="w-full overflow-hidden rounded-[0.5rem] md:rounded-[1rem] min-h-[20rem]" loading="lazy"
                    referrerpolicy="no-referrer-when-downgrade"></iframe>
            </div>
        </section>

        <%- include('partials/footer' , {flash,dataLegal,dataSocial,dataSocial,showContactModal:false}) %>